//  General
//  ───────────────────────────────────

{{#each general as |color|}}
oc-{{color.name}} = {{color.hex}}
{{/each}}
{{#each colors as |color|}}


//  {{capitalize color.name}}
//  ───────────────────────────────────

oc-{{color.name}}-list = {
{{#each color.hex as |hex index|}}
  '{{index}}': {{hex}}{{#unless @last}},{{/unless}}
{{/each}}
}

oc-{{color.name}}-0 = oc-{{color.name}}-list['0']
oc-{{color.name}}-1 = oc-{{color.name}}-list['1']
oc-{{color.name}}-2 = oc-{{color.name}}-list['2']
oc-{{color.name}}-3 = oc-{{color.name}}-list['3']
oc-{{color.name}}-4 = oc-{{color.name}}-list['4']
oc-{{color.name}}-5 = oc-{{color.name}}-list['5']
oc-{{color.name}}-6 = oc-{{color.name}}-list['6']
oc-{{color.name}}-7 = oc-{{color.name}}-list['7']
oc-{{color.name}}-8 = oc-{{color.name}}-list['8']
oc-{{color.name}}-9 = oc-{{color.name}}-list['9']
{{/each}}